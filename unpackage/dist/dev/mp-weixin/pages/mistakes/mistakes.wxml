<view class="concent"><block wx:if="{{activeE=='1'}}"><view><view class="tab-bar"><block wx:if="{{tabTList.total>'0'}}"><view data-event-opts="{{[['tap',[['toggleTab',[0]]]]]}}" class="{{['tab-list',(tabIndex=='0')?'active':'']}}" bindtap="__e">{{"全部"+tabTList.total}}</view></block><block wx:if="{{tabTList.singleQuestionCount>'0'}}"><view data-event-opts="{{[['tap',[['toggleTab',[1]]]]]}}" class="{{['tab-list',(tabIndex=='1')?'active':'']}}" bindtap="__e">{{"单选"+tabTList.singleQuestionCount}}</view></block><block wx:if="{{tabTList.multipleQuestionCount>'0'}}"><view data-event-opts="{{[['tap',[['toggleTab',[2]]]]]}}" class="{{['tab-list',(tabIndex=='2')?'active':'']}}" bindtap="__e">{{"多选"+tabTList.multipleQuestionCount}}</view></block><block wx:if="{{tabTList.judgeQuestionCount>'0'}}"><view data-event-opts="{{[['tap',[['toggleTab',[3]]]]]}}" class="{{['tab-list',(tabIndex=='3')?'active':'']}}" bindtap="__e">{{"判断"+tabTList.judgeQuestionCount}}</view></block><block wx:if="{{tabTList.gapQuestionCount>'0'}}"><view data-event-opts="{{[['tap',[['toggleTab',[4]]]]]}}" class="{{['tab-list',(tabIndex=='4')?'active':'']}}" bindtap="__e">{{"填空"+tabTList.gapQuestionCount}}</view></block></view><view class="tab-concent"><view class="mis-all"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="mistakes"><block wx:if="{{item.type=='1'}}"><view class="problem-t">{{index+1+".【单选题】"}}</view></block><block wx:else><block wx:if="{{item.type=='2'}}"><view class="problem-t">{{index+1+".【多选题】"}}</view></block><block wx:else><block wx:if="{{item.type=='3'}}"><view class="problem-t">{{index+1+".【判断题】"}}</view></block></block></block><view class="problem-title">{{''+item.content+''}}</view><view class="problem-option"><text>{{"A."+item.a}}</text><text>{{"B."+item.b}}</text><block wx:if="{{item.c!=null}}"><text>{{"C."+item.c}}</text></block><block wx:if="{{item.d!=null}}"><text>{{"D."+item.d}}</text></block></view><view class="problem-an">你的答案：<text class="yan">{{item.user_answer}}</text>正确答案：<text class="correct">{{item.answer}}</text></view></view></block></view></view></view></block><block wx:if="{{activeE=='0'}}"><view class="empty-all"><image class="empty-img" src="../../static/cuoti.png" mode></image><text>您还没有错题记录，继续保持努力！</text></view></block></view>