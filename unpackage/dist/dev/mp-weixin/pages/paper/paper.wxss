
.uni-page-head-btn{
	display: none;
}
.concent{
	padding: 100rpx;
}
.grade-c{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 100%;
	padding-bottom: 68rpx;
	overflow: hidden;
}
.grade-error,.grade-time{
	font-size:28rpx;
	color:#999;
}
.num{
	padding-left: 16rpx;
	color: #FF5A5D;
	font-size: 30rpx;
}
.dao{
	padding-left: 10rpx;
	color: #333333;
	font-size: 30rpx;
}
.grade-time text{
	padding-left: 16rpx;
	color: #333333;
	font-size: 30rpx;
}
.grade{
	background:url(/static/grader.png) no-repeat center top;
	background-size: contain;
	width: 400rpx;
	height: 400rpx;
	margin: auto;
	padding-top: 55rpx;
	text-align: center;
	font-size:30rpx;
	color:#ffffff99;
}
.grade-num{
	display: block;
	width: 100%;
	margin-top: -40rpx;
	font-size:125rpx;
	color:#fff;
	overflow: hidden;
}
.grade-b{
	width: 100%;
	padding-top: 60rpx;
	text-align: center;
}
.grade-b view{
	width:348rpx;
	height:82rpx;
	font-size: 34rpx;
	line-height: 78rpx;
	margin: 0 auto 50rpx;
	border-radius:42rpx;
}
.grade-see{
	color: #fff;
	background:#17A1E8;
	border:1px solid #17A1E8;
}
.grade-start{
	color: #17A1E8;
	background:#fff;
	border:1px solid #17A1E8;
}
.grade-d{
	text-align: center;
	font-size:25rpx;
	color:#666;
}


