
.concent{
}
.page-head{
	position: fixed;
	top: 0;
	z-index: 998;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width:100%;
	height:88rpx;
	font-size: 36rpx;
	text-align: center;
	background:#FEFEFE;
	-webkit-box-shadow:0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.35);
	        box-shadow:0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.35)
}
.uni-page-head-btn{
	position: absolute;
	top:0;
	left: 0;
	line-height: 104rpx;
}
.page-head-b{
	font-weight: 400;
	font-size: 54rpx;
	color: #000000;
}
.login-img{
	width: 408rpx;
	height: 394rpx;
	overflow: hidden;
}
.login-form{
	width: 100%;
	padding-top: 32rpx;
	text-align: left;
	overflow: hidden;
}
.login-text{
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 100%;
	height: 90rpx;
	text-align: left;
	border-bottom: 1px solid #BDBDBD;
	overflow: hidden;
}
.login{
	padding: 200rpx 132rpx;
	text-align: center;
}
.login-text ._i{
	font-size: 46rpx;
	color: #2878FF;
}
.login-text input{
	width: 300rpx;
	padding-left: 20rpx;
	font-size: 30rpx;
	color: #999;
}
.login-btn{
	width:100%;
	height:84rpx;
	margin-top: 60rpx;
	font-size: 38rpx;
	color: #fff;
	line-height: 82rpx;
	text-align: center;
	background:#2878FF;
	border:1px solid #17A1E8;
	border-radius:42px;
}
.login-text text{
	display: inline-block;
	width:164rpx;
	font-size:30rpx;
	font-weight:300;
	color:#2878FF;	
	text-align: right;
}

