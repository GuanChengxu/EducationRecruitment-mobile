<view class="concent"><view class="page-sign"><view class="page-h-title">报名信息</view><view class="page-s-con"><view hidden="{{!(nameE=='0')}}" class="sign"><label for>姓名</label><input class="sign-text" type="text" value="{{name}}" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" bindinput="__e"/></view><view hidden="{{!(telE=='0')}}" class="sign"><label for>手机号</label><input class="sign-text" type="number" value="{{tel}}" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" bindinput="__e"/></view><view hidden="{{!(cardE=='0')}}" class="sign"><label for>身份证</label><input class="sign-text" type="text" value="{{card}}" placeholder="请输入身份证" data-event-opts="{{[['input',[['__set_model',['','card','$event',[]]]]]]}}" bindinput="__e"/></view><view hidden="{{!(jobE=='0')}}" class="sign"><label for>工号</label><input class="sign-text" type="text" value="{{job}}" placeholder="请输入工号" data-event-opts="{{[['input',[['__set_model',['','job','$event',[]]]]]]}}" bindinput="__e"/></view><view hidden="{{!(sectionE=='0')}}" class="sign"><label for>部门</label><input class="sign-text" type="text" value="{{section}}" placeholder="请输入部门" data-event-opts="{{[['input',[['__set_model',['','section','$event',[]]]]]]}}" bindinput="__e"/></view><view hidden="{{!(dutyE=='0')}}" class="sign"><label for>职务</label><input class="sign-text" type="text" value="{{duty}}" placeholder="请输入职务" data-event-opts="{{[['input',[['__set_model',['','duty','$event',[]]]]]]}}" bindinput="__e"/></view></view><view class="start-btn" type="primary" data-event-opts="{{[['tap',[['startBtn',['$event']]]]]}}" bindtap="__e">开始考试</view></view></view>