<view class="concent"><view class="login"><image class="login-img" src="../../static/login-img.png" mode></image><view class="login-form"><view class="login-text"><view class="iconfont icon icon-zhanghao _i"></view><input type="number" value="{{tel}}" placeholder="请输入电话号" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" bindinput="__e"/></view><view class="login-text"><view class="iconfont icon icon-mima _i"></view><input type="password" value="{{pwd}}" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','pwd','$event',[]]]]]]}}" bindinput="__e"/></view><view class="login-btn" type="primary" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">登录</view><view data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="register" bindtap="__e">免费注册</view></view></view></view>